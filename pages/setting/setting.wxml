<!--setting.wxml-->
<view class="container">
  <view class="userinfo">
  <open-data class="userinfo-avatar" type="userAvatarUrl" background-size="cover"></open-data>
<open-data class="userinfo-nickname" type="userNickName"></open-data>
  </view>
    <view bindtap="chooseMusic" class="option">
    <image src="../../image/sugar.png" class="iconPre"></image>设置音乐<image src="../../image/right.png" class="right"></image></view>
    <view bindtap="chooseBeauty" class="option">
    <image src="../../image/beauty.png" class="iconBeauty"></image>设置人物<image src="../../image/right.png" class="right"></image></view>
  <view bindtap="addTemplate" class="option">
  <image src="../../image/template.png" class="iconPre"></image>自定义模板<image src="../../image/right.png" class="right"></image></view>
</view>